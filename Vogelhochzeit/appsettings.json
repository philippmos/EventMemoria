{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
    "AllowedHosts": "*",
    "ImageOptions": {
        "ContainerName": "images",
        "AllowedFileTypes": [
            ".jpg",
            ".jpeg",
            ".png",
            ".gif",
            ".webp",
            ".bmp"
        ],
        "DefaultPhotosPerRow":  2
    }
}
